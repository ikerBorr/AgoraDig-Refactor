{
  "name": "prueba",
  "scripts": {
    "generate:routes": "bun src/app/route-generator.js",
    "dev": "bun run generate:routes && bun --watch src/app/start.ts",
    "lint": "bun x eslint \"src/**/*.{ts,tsx,js}\" \"test/**/*.{ts,tsx,js}\"",
    "lint:fix": "bun x eslint \"src/**/*.{ts,tsx,js}\" \"test/**/*.{ts,tsx,js}\" --fix",
    "format": "bun x prettier -w src test",
    "format:check": "bun x prettier --check src test",
    "typecheck": "tsc --noEmit",
    "test": "bun x vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "clean": "bun x rimraf dist",
    "prebuild": "bun run clean && bun run generate:routes && bun run lint && bun run test",
    "build": "tsc -p tsconfig.json",
    "start:node": "node --enable-source-maps dist/src/app/Start.js",
    "start:bun": "bun dist/Start.js",
    "docker:build": "docker-compose build",
    "docker:restart": "docker-compose restart",
    "docker:dev": "docker compose up",
    "docker:clean": "docker-compose down -v",
    "docker:rebuild": "docker-compose down && docker-compose build --no-cache && docker-compose up -d"
  },
  "dependencies": {
    "@types/express-session": "^1.18.2",
    "axios": "^1.12.2",
    "compression": "^1.8.1",
    "connect-mongo": "^5.1.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "express-session": "^1.18.2",
    "fast-glob": "^3.3.3",
    "helmet": "^8.1.0",
    "inversify": "^7.10.2",
    "jose": "^6.1.0",
    "mongoose": "^8.19.0",
    "morgan": "^1.10.1",
    "reflect-metadata": "^0.2.2",
    "sharp": "^0.34.4",
    "swagger-ui-express": "^5.0.1",
    "yamljs": "^0.3.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@faker-js/faker": "^10.0.0",
    "@types/bun": "latest",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/morgan": "^1.9.10",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/yamljs": "^0.2.34",
    "eslint": "^9.37.0",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "typescript-eslint": "^8.45.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  }
}
